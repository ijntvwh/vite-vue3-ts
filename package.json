{
  "name": "vite-vue3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "types-sync && husky install",
    "lint": "npm run lint:eslint && npm run lint:prettier && npm run lint:style",
    "lint:eslint": "eslint --max-warnings 0 \"{src,types}/**/*.{vue,js,jsx,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write \"{src,types}/**/*.{vue,js,jsx,ts,tsx,json,css,scss,html,md}\"",
    "lint:style": "stylelint \"**/*.{html,vue,sass,scss,css}\" --fix"
  },
  "lint-staged": {
    "*.{vue,ts,js}": [
      "npm run lint"
    ],
    "*.{html,vue,sass,scss,css}": [
      "npm run lint:style"
    ]
  },
  "dependencies": {
    "@unocss/reset": "^0.58.3",
    "axios": "^1.6.5",
    "dayjs": "^1.11.10",
    "lodash-es": "^4.17.21",
    "naive-ui": "^2.37.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "vue": "^3.4.14",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/get-folder-size": "^3.0.4",
    "@types/lodash-es": "^4.17.12",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitejs/plugin-vue-jsx": "3.1.0",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "12.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.20.1",
    "filesize": "^10.1.0",
    "get-folder-size": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.70.0",
    "stylelint": "^16.1.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "types-sync": "^3.69.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-icons": "^0.18.2",
    "unplugin-vue-components": "^0.26.0",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^5.0.11",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-inspect": "^0.8.1",
    "vite-plugin-stylelint": "^5.3.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-svg-loader": "5.1.0",
    "vue-eslint-parser": "9.4.0",
    "vue-tsc": "^1.8.27"
  }
}
